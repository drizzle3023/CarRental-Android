<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.drizzle.carrental">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"/>
        <activity android:name=".OnboardingActivity"/>
        <activity android:name=".MyCoverageEmptyActivity"/>
        <activity android:name=".SubscriptionNewActivity"/>
        <activity android:name=".SignUpLoginActivity"/>
        <activity android:name=".VerifyCodeActivity"/>
        <activity android:name=".CoverageActivity"/>
        <activity android:name=".SubscribeSuccessActivity"/>
        <activity android:name=".PaymentActivity"/>

    </application>

</manifest>
