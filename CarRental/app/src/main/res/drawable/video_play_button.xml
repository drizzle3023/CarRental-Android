<?xml version="1.0" encoding="utf-8"?>
<layer-list>
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <padding android:bottom="11dp"
                android:left="11dp"
                android:right="11dp"
                android:top="11dp"/>

            <solid android:color="#55FFFFFF"/>
        </shape>
    </item>

    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">

            <gradient
                android:startColor="#AA4EDB"
                android:endColor="#7126B4"
                android:angle="180"/>
        </shape>
    </item>
</layer-list>

