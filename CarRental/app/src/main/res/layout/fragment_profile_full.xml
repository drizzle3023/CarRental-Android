<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentRight="true"
        android:fontFamily="@font/sf_pro_display_semibold"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/account_title"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textSize="17sp" />

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"
        android:background="@color/colorDivider" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">



            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileCategoryCaption"
                android:layout_marginLeft="23dp"
                android:text="Profile" />

            <TextView
                android:id="@+id/textview_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileItem"
                android:layout_marginLeft="23dp"
                android:text="John" />

            <TextView
                android:id="@+id/textview_phonenumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileItem"
                android:layout_marginLeft="23dp"
                android:paddingTop="5dp"
                android:text="" />

            <TextView
                android:id="@+id/link_add_email"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginLeft="23dp"
                style="@style/AppTheme.ProfileButton"
                android:paddingTop="5dp"
                android:text="+ Add email" />

            <View
                android:id="@+id/view7"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/colorDivider" />

            <TextView
                android:id="@+id/textView22"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileCategoryCaption"
                android:layout_marginLeft="23dp"
                android:layout_marginTop="10dp"
                android:text="Support" />

            <TextView
                android:id="@+id/link_feedback"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:textColorLink="@android:color/holo_blue_dark"
                android:text="Feedback" />

            <TextView
                android:id="@+id/link_faqs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:textColorLink="@android:color/holo_blue_dark"
                android:paddingTop="5dp"
                android:text="FAQs" />

            <View
                android:id="@+id/view8"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/colorDivider" />

            <TextView
                android:id="@+id/textView24"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileCategoryCaption"
                android:layout_marginLeft="23dp"
                android:text="Payment" />

            <TextView
                android:id="@+id/link_card_no"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:text="****4242" />

            <View
                android:id="@+id/view8_2"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:visibility="gone"
                android:background="@color/colorDivider" />

            <TextView
                android:id="@+id/textView24_2"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileCategoryCaption"
                android:visibility="gone"
                android:layout_marginLeft="23dp"
                android:text="Settings" />

            <TextView
                android:id="@+id/link_permissions"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:visibility="gone"
                android:text="Permissions" />

            <View
                android:id="@+id/view8_3"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_alignParentBottom="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/colorDivider" />

            <TextView
                android:id="@+id/textView24_3"
                android:layout_width="match_parent"
                android:layout_height="41dp"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileCategoryCaption"
                android:layout_marginLeft="23dp"
                android:text="App" />

            <TextView
                android:id="@+id/link_about"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:textColorLink="@android:color/holo_blue_dark"
                android:text="About" />

            <TextView
                android:id="@+id/link_logout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                style="@style/AppTheme.ProfileButton"
                android:layout_marginLeft="23dp"
                android:paddingTop="5dp"
                android:text="Log out" />

        </LinearLayout>
    </ScrollView>



</LinearLayout>